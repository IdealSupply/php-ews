{
  "name": "idealsupply/php-ews",
  "description": "Library for communicating with Exchange Web Services.",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "ext-soap": "*",
    "idealsupply/php-ntlm": "~1.0"
  },
  "require-dev": {
    "mustache/mustache": "~2.0",
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "@stable",
    "phan/phan": "@stable"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "jamesiarmes\\PhpEws\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.idealsupply.com"
    }
  ],
}
